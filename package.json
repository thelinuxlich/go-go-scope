{
	"name": "go-go-scope",
	"version": "1.5.2",
	"description": "Structured concurrency for TypeScript using Explicit Resource Management",
	"type": "module",
	"main": "./dist/index.mjs",
	"types": "./dist/index.d.mts",
	"exports": {
		".": {
			"types": "./dist/index.d.mts",
			"default": "./dist/index.mjs"
		},
		"./testing": {
			"types": "./dist/testing/index.d.mts",
			"default": "./dist/testing/index.mjs"
		},
		"./persistence": {
			"types": "./dist/persistence/index.d.mts",
			"default": "./dist/persistence/index.mjs"
		},
		"./persistence/redis": {
			"types": "./dist/persistence/redis.d.mts",
			"default": "./dist/persistence/redis.mjs"
		},
		"./persistence/sqlite": {
			"types": "./dist/persistence/sqlite.d.mts",
			"default": "./dist/persistence/sqlite.mjs"
		},
		"./persistence/postgres": {
			"types": "./dist/persistence/postgres.d.mts",
			"default": "./dist/persistence/postgres.mjs"
		},
		"./persistence/mysql": {
			"types": "./dist/persistence/mysql.d.mts",
			"default": "./dist/persistence/mysql.mjs"
		}
	},
	"scripts": {
		"build": "pkgroll",
		"lint": "biome check --write src/",
		"test": "npm run build && npm run lint && vitest run",
		"test:watch": "vitest",
		"test:integration": "vitest run tests/persistence-integration.test.ts",
		"test:integration:watch": "vitest tests/persistence-integration.test.ts",
		"test:bun": "bun test tests/bun-compatibility.test.ts",
		"test:bun:all": "bun test",
		"benchmark": "npm run build && node --experimental-strip-types benchmarks/index.ts",
		"services:up": "docker compose up -d redis postgres mysql",
		"services:down": "docker compose down redis postgres mysql",
		"services:logs": "docker compose logs -f redis postgres mysql",
		"jaeger:up": "docker compose up -d jaeger",
		"jaeger:down": "docker compose down jaeger",
		"example:jaeger": "npm run build && node examples/jaeger-tracing.ts",
		"prometheus:up": "docker compose up -d prometheus pushgateway grafana",
		"prometheus:down": "docker compose down prometheus pushgateway grafana",
		"example:prometheus": "npm run build && node --experimental-strip-types examples/prometheus-metrics.ts",
		"example:prometheus:push": "npm run build && node --experimental-strip-types examples/prometheus-push.ts",
		"monitoring:up": "docker compose up -d",
		"monitoring:down": "docker compose down",
		"prepublishOnly": "npm test",
		"publish:patch": "npm version patch && npm publish",
		"publish:minor": "npm version minor && npm publish",
		"publish:major": "npm version major && npm publish"
	},
	"keywords": [
		"structured-concurrency",
		"async",
		"disposable",
		"using",
		"typescript",
		"esm",
		"es2022"
	],
	"engines": {
		"node": ">=18.0.0"
	},
	"author": "thelinuxlich",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/thelinuxlich/go-go-scope.git"
	},
	"bugs": {
		"url": "https://github.com/thelinuxlich/go-go-scope/issues"
	},
	"homepage": "https://github.com/thelinuxlich/go-go-scope#readme",
	"files": [
		"dist/",
		"README.md",
		"LICENSE"
	],
	"devDependencies": {
		"@biomejs/biome": "^2.4.2",
		"@opentelemetry/exporter-trace-otlp-http": "^0.212.0",
		"@opentelemetry/resources": "^2.5.1",
		"@opentelemetry/sdk-node": "^0.212.0",
		"@opentelemetry/semantic-conventions": "^1.39.0",
		"@types/debug": "^4.1.12",
		"@types/node": "^24",
		"effect": "^3.19.17",
		"go-go-try": "^7.3.0",
		"ioredis": "^5.9.3",
		"mysql2": "^3.17.2",
		"pg": "^8.18.0",
		"pkgroll": "^2.26.0",
		"sqlite3": "^5.1.7",
		"typescript": "^5.9.3",
		"vitest": "^4.0.18"
	},
	"dependencies": {
		"@opentelemetry/api": "^1.9.0",
		"debug": "^4.4.3"
	},
	"peerDependencies": {
		"ioredis": "^5.0.0",
		"mysql2": "^3.6.0",
		"pg": "^8.11.0"
	}
}
