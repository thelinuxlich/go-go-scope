{
	"name": "go-go-scope",
	"version": "1.0.0",
	"description": "Structured concurrency for TypeScript using Explicit Resource Management",
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.mts",
	"exports": {
		"require": {
			"types": "./dist/index.d.cts",
			"default": "./dist/index.cjs"
		},
		"import": {
			"types": "./dist/index.d.mts",
			"default": "./dist/index.mjs"
		}
	},
	"type": "module",
	"scripts": {
		"build": "pkgroll",
		"lint": "biome check --write src/",
		"test": "npm run build && npm run lint && vitest run",
		"test:watch": "vitest",
		"jaeger:up": "docker compose up -d",
		"jaeger:down": "docker compose down",
		"example:jaeger": "npm run build && node examples/jaeger-tracing.ts"
	},
	"keywords": [
		"structured-concurrency",
		"async",
		"disposable",
		"using",
		"typescript"
	],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@biomejs/biome": "^2.3.14",
		"@opentelemetry/exporter-trace-otlp-http": "^0.211.0",
		"@opentelemetry/resources": "^2.5.0",
		"@opentelemetry/sdk-node": "^0.211.0",
		"@opentelemetry/semantic-conventions": "^1.39.0",
		"@types/debug": "^4.1.12",
		"@types/node": "^24",
		"pkgroll": "^2.24.0",
		"typescript": "^5.9.3",
		"vitest": "^4.0.18"
	},
	"peerDependencies": {
		"go-go-try": "^6.0.0"
	},
	"dependencies": {
		"@opentelemetry/api": "^1.9.0",
		"debug": "^4.4.3"
	}
}
